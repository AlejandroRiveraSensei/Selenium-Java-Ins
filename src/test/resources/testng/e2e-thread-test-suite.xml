<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="E2ETests" parallel="classes" thread-count="3">

    <test name="OnlyE2EGroup">
        <groups>
            <run>
                <include name="e2e"/>
            </run>
        </groups>
        <classes>
            <class name="pom.LoginTests"/>
            <class name="pom.InventoryTest"/>
            <class name="pom.CheckoutTests"/>
        </classes>
    </test>
</suite>


